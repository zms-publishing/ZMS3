<style>
table.table.zmi-sortable tr.zmi-sortable .ui-toggle {
	visibility:hidden;
}
table.table.zmi-sortable tr.zmi-sortable.active {
	background-color:#ffc!important;
}
table.table.zmi-sortable tr.zmi-sortable.active .ui-toggle {
	visibility:visible;
}
.horizontal-group .control-group {
	float:left;
	margin-right:0.5em;
}
.horizontal-group .mandatory span {
	background: none !important;
	padding-left: 0 !important;
}
.horizontal-group .controls {
	margin-left: 0px;
}
.horizontal-group .control-group label {
	float:none;
	width:auto;
	text-align:left;
}
.horizontal-group .controls.save {
	margin-top:2.1em;
}

.zmi-image-preview {
	margin-left:1em;
	border:1px solid black;
}
div.activity {
	white-space: nowrap;
	border: 1px solid maroon;
	border-radius:5px;
	background-color: #FFFFE0;
	padding:2px;
	text-align:center;
	margin-top:1px;
	margin-bottom:1px;
}
div.transition {
	border: 1px solid navy;
	border-radius:5px;
	background-color:#D0FFD0;
	padding-left:2px;
	padding-right:2px;
	padding-top:0px;
	padding-bottom:0px;
	text-align:center;
	width: 170px;
}
div.transition.state {
	background-color:#D0D0FF;
}
div.transition.action {
	background-color:#FFD0D0;
}
div.activity a,
div.transition a {
	color:black;
}
.center {
	text-align:center;
}
.smaller {
	font-size:smaller;
	width: 160px;
	overflow:hidden;
}
.layoutTable th, .layoutTable td {
	padding:0;
	vertical-align:middle;
	border:0;
}
ul.performer {
	list-style:none;
	padding:2px;
	margin:2px;
	background-color:#FFF;
	background: filter:Alpha(opacity=50);-moz-opacity:0.50;opacity:0.50;
}
</style>

<script>

/**
 * Delete object.
 */
function zmiDeleteObjBtnClick(sender,d) {
	if (confirm(getZMILangStr('MSG_CONFIRM_DELOBJ'))) {
		zmiFormSubmit(sender,d);
	}
}

/**
 * Submit form with given parameters.
 */
function zmiFormSubmit(sender,d) {
	var $fm = $(sender).closest("form");
	var html = '';
	for (var i in d) {
		$('input[name="' + i + '"]',$fm).remove();
		html += '<input type="hidden" name="' + i + '" value="' + d[i] +'"/>';
	}
	$fm
		.append(html)
		.submit();
}

$(function() {
		$('table.zmi-sortable tr').hover(
			function() {
					$(this).addClass('active');
				},
			function() {
					$(this).removeClass('active');
				});
		$('#updateActivity').dialog({modal:true,title:getZMILangStr('TAB_PROPERTIES')+': Activity',height:'auto',width:'auto'});
		$('#updateTransition').dialog({modal:true,title:getZMILangStr('TAB_PROPERTIES')+': Transition',height:'auto',width:'auto'});
	});

</script>

