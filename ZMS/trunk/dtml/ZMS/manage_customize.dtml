################################################################################
</dtml-comment>
<dtml-if "REQUEST.get('key')=='Maintenance'">
<dtml-if "REQUEST.get('btn','').startswith('Reindex')">
<div class="system-msg"><b>Catalog:</b> Reindex<br/><dtml-var "reindexCatalog(REQUEST,clients='catalog_portal_clients' in REQUEST.form.get('options',[]))"><hr/>(<dtml-var "getLangFmtDate(ZopeTime())">)</div>
<dtml-elif "REQUEST.get('btn','').startswith('Correct')">
<div class="system-msg"><b>Internal links:</b> Correct<br/><dtml-var "synchronizeRefs(clients='internal_links_portal_clients' in REQUEST.form.get('options',[]),unify_ids='internal_links_unify_ids' in REQUEST.form.get('options',[]))"><hr/>(<dtml-var "getLangFmtDate(ZopeTime())">)</div>
<dtml-elif "REQUEST.get('btn','').startswith('Clear')">
<div class="system-msg"><b>Static cache:</b> Clear<br/><dtml-var "clearCachePages()"><hr/>(<dtml-var "getLangFmtDate(ZopeTime())">)</div>
