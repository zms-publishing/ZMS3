 <dtml-call "setConfProperty('ZMS.CatalogAwareness.active',REQUEST.get('catalog_awareness_active')==1)">
 <dtml-call "setConfProperty('ZMS.InternalLinks.autocorrection',REQUEST.get('linkcorrection_active')==1)">
