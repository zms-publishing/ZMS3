<!DOCTYPE html>
<html lang="en">
<head>
<!-- ZMS CUSTOM HEADER -->
<dtml-unless management_page_charset>
<dtml-call "REQUEST.set('management_page_charset','utf-8')">
</dtml-unless>
<meta http-equiv="content-type" content="text/html;charset=&dtml-management_page_charset;" />
<dtml-call "RESPONSE and RESPONSE.setHeader('content-type','text/html;charset='+management_page_charset)">
<title><dtml-if title_or_id><dtml-var title_or_id><dtml-else>Zope</dtml-if></title>

<script type="text/javascript" charset="UTF-8" src="/++resource++zms_/zmi.js"></script>
<script type="text/javascript" charset="UTF-8" src="/++resource++zms_/i18n/eng.js"></script>
<script type="text/javascript" charset="UTF-8" src="/++resource++zms_/bootstrap/bootstrap-all.min.js"></script>

<link rel="stylesheet" type="text/css" href="&dtml-BASEPATH1;/manage_page_style.css" />

</head>
<body id="nodeid_<dtml-var "(REQUEST['URL1'].split(REQUEST['BASE0'])[1]).split('/')[-1]">">
<dtml-call "REQUEST.set('used_zms_custom_manage_page_header',True)">

<dtml-comment>
<!--  EXPERIMENTAL: IFRAMED TREE MENU -->
<div id="zminav" style="
		position: fixed;
		z-index: 1000;
		right: 0;
">
<a href="#" onclick="javascript:toggle_menu()" style="
		position: fixed;
		color: silver;
		right: .5em;
		z-index: 1001;
		font-size: 120%;
		display: inline-block;
		top: 0.5em;
	"><i class="icon icon-reorder"
		title="Show/Hide Tree Menu">&nbsp;</i></a>
	<iframe style="
		position: fixed;
		width:38%;
		height: 100%;
		right: 0px;
		box-shadow: black 0px 0px 20px;
		border-width: 0px 0px 0px 1px;
		border-left-style: solid;
		border-left-color: silver;
		background: black;
		display:none;
		" id="iframe_menu">iframe_menu</iframe>
	<script>
		function toggle_menu() {
			$('#iframe_menu').toggle();
			if ( $('#iframe_menu').css('display') != 'none' && !$('#iframe_menu').attr('src') ) {
				$('#iframe_menu').attr('src','/manage_menu');
			};
		}
	</script>
</div>
</dtml-comment>

